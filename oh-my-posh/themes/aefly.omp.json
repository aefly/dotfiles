{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "black": "#111111",
    "white": "#f7f7f7"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u250c",
          "foreground": "p:white",
          "background": "transparent",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "linux": "\uf17c"
          },
          "template": " {{.Icon}}{{if .WSL}} WSL{{end}}\u2800",
          "foreground": "p:white",
          "background": "#3950ae",
          "type": "os",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf489 {{.Name}}\u2800",
          "foreground": "p:white",
          "background": "#3939ae",
          "type": "shell",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf2c0 {{.UserName}}\u2800",
          "foreground": "p:white",
          "background": "#3950ae",
          "type": "session",
          "style": "plain"
        },
        {
          "background": "#3939ae",
          "foreground": "p:white",
          "properties": {
            "branch_icon": " \ue702 "
          },
          "style": "plain",
          "template": "{{.HEAD}}\u2800",
          "type": "git"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_package_manager": true,
            "pnpm_icon": "\ue865"
          },
          "type": "node",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "#3939ae",
          "template": " {{ .PackageManagerIcon }} {{ .Full }} "
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ end }} ",
          "foreground": "p:white",
          "background": "#3939ae",
          "type": "python",
          "style": "plain"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\uf07b",
            "folder_separator_template": "<#4a67d9> Â» </>",
            "home_icon": "\ueb06",
            "style": "agnoster"
          },
          "template": "<p:white>\u2514</><#4a67d9>[</> {{.Path}} <#4a67d9>]</>",
          "foreground": "p:white",
          "background": "transparent",
          "type": "path",
          "style": "plain"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "transparent",
          "foreground": "p:white",
          "foreground_templates": ["{{if gt .Code 0}}p:white{{end}}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u276f",
          "type": "status"
        }
      ]
    }
  ],
  "console_title_template": "{{if .Root}}[root] {{end}}{{.Shell}} in <{{.Folder}}>",
  "final_space": true,
  "version": 3,
  "auto_upgrade": true
}
